{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [         
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**"
        ],
        "src": "articles",
        "dest": "."
      },
      {
        "files": [
          "bread/toc.yml"
        ],
        "src": null,
        "dest": null
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/_themes/**"
        ],
        "src": "articles",
        "dest": "."
      },
      {
        "files": [
          "includes/**/*.png",
          "includes/**/*.jpg",
          "includes/**/*.jpeg",
          "includes/**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/_themes/**"
        ],
        "src": null,
        "dest": null
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/azure/bread/toc.json"
    },
    "fileMetadata": {},
    "template": [
      "docs.html"
    ],
    "dest": "azure"
  }
}
